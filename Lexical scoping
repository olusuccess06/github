makecachematrix<- function (x = matrix()){
inv<- NULL
set<-function(y){
x<<-y
inv<<-NULL
}
get<-function() x
setInverse<- function() inv<<-solve(x)
getInverse<- function() inv
list(set = set,
get = get,
setInverse = setInverse,
getInverse = getInverse)
}
cachemean<- function (x,...) {
inv<-x$getInverse()
if(!is.null(inv)){
message("getting cached data")
return(inv)
}
data <-x$get()
inv <- Inverse(data,...)
x$setInverse(inv)
inv
}
